    gridline-color: #d0d0d0; /* 网格线颜色 */
    border-radius: 6px; /* 边角圆润 */
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2); /* 更加明显的阴影效果 */
}

QTableWidget::item {
    padding: 6px; /* 单元格内边距 */
    color: #2c2c2c; /* 字体颜色 */
    border-radius: 4px; /* 单元格边角圆润 */
    background-color: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0 #ffffff, stop: 1 #f5f5f5
    ); /* 水平渐变背景 */
}

QHeaderView::section {
    background-color: qlineargradient(
        x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 #dcdcdc, stop: 1 #c0c0c0
    ); /* 表头垂直渐变背景 */
    color: #2c2c2c; /* 表头字体颜色 */
    border: 1px solid #ccc; /* 表头边框颜色 */
    border-radius: 4px; /* 表头边角圆润 */
    padding: 6px; /* 表头内边距 */
    font-weight: bold; /* 字体加粗 */
}

QTableWidget::item:selected {
    background-color: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0 #b3d9ff, stop: 1 #a0c8ff
    ); /* 选中单元格水平渐变背景 */
